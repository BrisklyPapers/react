'use strict';var _react = require('react');var _react2 = _interopRequireDefault(_react);
var _search = require('../../src/actions/search.js');
var _searchedText = require('../../src/reducers/searchedText');function _interopRequireDefault(obj) {return obj && obj.__esModule ? obj : { default: obj };}

describe('reducers/searchedText', function () {
    it('returns an empty state if no input is given', function () {
        expect(
        (0, _searchedText.searchedText)("", { type: _search.SEARCH_INPUT_KEY_PRESSED, text: "" })).
        toEqual(
        "");

    });
    it('returns typed text after key press', function () {
        expect(
        (0, _searchedText.searchedText)("a", { type: _search.SEARCH_INPUT_KEY_PRESSED, text: "ab" })).
        toEqual(
        "ab");

    });
    it('returns the default state', function () {
        expect(
        (0, _searchedText.searchedText)(undefined, { type: "" })).
        toEqual(
        "");

    });
    it('returns the current state if action is not valid', function () {
        expect(
        (0, _searchedText.searchedText)("f", { type: "" })).
        toEqual(
        "f");

    });
});