'use strict';var _search = require('../../src/actions/search.js');
var _searchedText = require('../../src/reducers/searchedText');

describe('reducers/searchedText', function () {
    it('no input given', function () {
        expect(
        (0, _searchedText.searchedText)("", { type: _search.SEARCH_INPUT_KEY_PRESSED, text: "" })).
        toEqual(
        "");

    });
    it('key pressed', function () {
        expect(
        (0, _searchedText.searchedText)("a", { type: _search.SEARCH_INPUT_KEY_PRESSED, text: "ab" })).
        toEqual(
        "ab");

    });
    it('unkown action + default state', function () {
        expect(
        (0, _searchedText.searchedText)(undefined, { type: "" })).
        toEqual(
        "");

    });
});